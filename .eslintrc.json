{
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "extends": [
        "@ecomfe/eslint-config",
        "@ecomfe/eslint-config/typescript",
        "@ecomfe/eslint-config/react",
        "plugin:jest/recommended"
    ],
    "rules": {
        "react/no-array-index-key": "off",
        "comma-dangle": "off",
        "no-use-before-define": [0],
        "@typescript-eslint/no-use-before-define": [1]
    }
}